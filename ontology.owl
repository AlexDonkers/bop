@prefix : <http://www.github.com/AlexDonkers/bop#> .
@prefix bop: <http://www.github.com/AlexDonkers/bop#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix schema: <http://schema.org/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://www.github.com/AlexDonkers/bop#> .

<http://www.github.com/AlexDonkers/bop#> rdf:type owl:Ontology ;
                                          dcterms:creator <https://orcid.org/0000-0002-8809-3277> ;
                                          dcterms:description """The Building Performance Ontology (BOP) is a set of ontological constructs which allows integration of data related to building performance. By (re-)using ontology patterns from major ontologies in the field, BOP provides a method to integrate both static and dynamic (time-series) properties to buildings, zones, elements and products. 

It's aim is to facilitate building managers and facility managers with a common data structure to ease the process of data integration and to allow software tools to deal with the large heterogeneity of data. 

By (re-)using concepts from major ontologies in the field, BOP could easily be integrated with a wide range of existing ontologies and datasets. BOP could be seen as an extension of existing work rather than an individual ontology."""@en ;
                                          dcterms:title "Building Performance Ontology"@en ;
                                          vann:preferredNamespacePrefix "bop" ;
                                          vann:preferredNamespaceUri <http://www.github.com/AlexDonkers/bop#> ;
                                          owl:versionInfo 1.1 .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/contributor
dcterms:contributor rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
dcterms:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
dcterms:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#example
<http://www.w3.org/2004/02/skos/core#example> rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/name
foaf:name rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.github.com/AlexDonkers/bop#actsOn
bop:actsOn rdf:type owl:ObjectProperty ;
           owl:inverseOf bop:isActedOnBy ;
           rdfs:domain bop:Executor ;
           rdfs:range bop:Property ;
           rdfs:label "acts on"@en .


###  http://www.github.com/AlexDonkers/bop#actuatedBy
bop:actuatedBy rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf bop:executedBy ;
               owl:inverseOf bop:performedActuation ;
               rdfs:domain bop:Actuation ;
               rdfs:range bop:Actuator ;
               rdfs:label "actuated by"@en .


###  http://www.github.com/AlexDonkers/bop#containsProperty
bop:containsProperty rdf:type owl:ObjectProperty ;
                     owl:inverseOf bop:isPartOfPropertySet ;
                     rdfs:domain bop:PropertySet ;
                     rdfs:range bop:Property ;
                     rdfs:label "contains property"@en .


###  http://www.github.com/AlexDonkers/bop#executedBy
bop:executedBy rdf:type owl:ObjectProperty ;
               owl:inverseOf bop:performedExecution ;
               rdfs:domain bop:Execution ;
               rdfs:range bop:Executor ;
               rdfs:label "executed by"@en .


###  http://www.github.com/AlexDonkers/bop#hasCollection
bop:hasCollection rdf:type owl:ObjectProperty ;
                  owl:inverseOf bop:isCollectionOf ;
                  rdfs:domain bop:Database ;
                  rdfs:range bop:Collection ;
                  rdfs:label "has collection"@en .


###  http://www.github.com/AlexDonkers/bop#hasComplexProperty
bop:hasComplexProperty rdf:type owl:ObjectProperty ;
                       owl:inverseOf bop:isComplexPropertyOf ;
                       rdfs:domain bop:FeatureOfInterest ;
                       rdfs:range bop:Result ;
                       rdfs:label "has complex property"@en .


###  http://www.github.com/AlexDonkers/bop#hasExternalDatabase
bop:hasExternalDatabase rdf:type owl:ObjectProperty ;
                        owl:inverseOf bop:isExternalDatabaseOf ;
                        rdfs:domain bop:Executor ;
                        rdfs:range bop:Database ;
                        rdfs:label "has external database"@en .


###  http://www.github.com/AlexDonkers/bop#hasFeatureOfInterest
bop:hasFeatureOfInterest rdf:type owl:ObjectProperty ;
                         owl:inverseOf bop:isFeatureOfInterestOf ;
                         rdfs:domain bop:Execution ;
                         rdfs:range bop:FeatureOfInterest ;
                         rdfs:label "has feature of interest"@en .


###  http://www.github.com/AlexDonkers/bop#hasProperty
bop:hasProperty rdf:type owl:ObjectProperty ;
                owl:inverseOf bop:isPropertyOf ;
                rdfs:domain bop:FeatureOfInterest ;
                rdfs:range bop:Property ;
                rdfs:label "has property"@en .


###  http://www.github.com/AlexDonkers/bop#hasPropertyState
bop:hasPropertyState rdf:type owl:ObjectProperty ;
                     owl:inverseOf bop:isPropertyStateOf ;
                     rdfs:domain bop:Property ;
                     rdfs:range bop:Result ;
                     rdfs:comment "Linking a property with a result"@en ;
                     rdfs:label "has property state"@en ;
                     <http://www.w3.org/2004/02/skos/core#example> """This object property is used to directly link a property to a property state. 

Example: Temperature -- hasPropertyState --> CurrentTemperatureState"""@en .


###  http://www.github.com/AlexDonkers/bop#hasResult
bop:hasResult rdf:type owl:ObjectProperty ;
              owl:inverseOf bop:isResultOf ;
              rdfs:domain bop:Execution ;
              rdfs:range bop:Result ;
              rdfs:label "has result"@en .


###  http://www.github.com/AlexDonkers/bop#hasSubActuator
bop:hasSubActuator rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf bop:hasSubExecutor ;
                   owl:inverseOf bop:isSubActuatorOf ;
                   rdfs:domain bop:Actuator ;
                   rdfs:range bop:Actuator ;
                   rdfs:label "has sub-actuator"@en .


###  http://www.github.com/AlexDonkers/bop#hasSubExecutor
bop:hasSubExecutor rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   owl:inverseOf bop:isSubExecutorOf ;
                   rdfs:domain bop:Executor ;
                   rdfs:range bop:Executor ;
                   rdfs:label "has sub-executor"@en .


###  http://www.github.com/AlexDonkers/bop#hasSubProperty
bop:hasSubProperty rdf:type owl:ObjectProperty ;
                   rdfs:domain bop:Property ;
                   rdfs:range bop:Property ;
                   rdfs:label "has sub-property"@en .


###  http://www.github.com/AlexDonkers/bop#hasSubSensor
bop:hasSubSensor rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf bop:hasSubExecutor ;
                 owl:inverseOf bop:isSubSensorOf ;
                 rdfs:domain bop:Sensor ;
                 rdfs:range bop:Sensor ;
                 rdfs:label "has sub-sensor"@en .


###  http://www.github.com/AlexDonkers/bop#hasUnit
bop:hasUnit rdf:type owl:ObjectProperty ;
            owl:inverseOf bop:isUnitOf ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain bop:Result ;
            rdfs:range [ rdf:type owl:Restriction ;
                         owl:onProperty bop:hasUnit ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass bop:Unit
                       ] ;
            rdfs:label "has unit"@en .


###  http://www.github.com/AlexDonkers/bop#hosts
bop:hosts rdf:type owl:ObjectProperty ;
          owl:inverseOf bop:isHostedBy ;
          rdfs:domain bop:Platform ;
          rdfs:range bop:Executor ;
          rdfs:label "hosts"@en .


###  http://www.github.com/AlexDonkers/bop#implementedBy
bop:implementedBy rdf:type owl:ObjectProperty ;
                  owl:inverseOf bop:implementsProcedure ;
                  rdfs:domain bop:Procedure ;
                  rdfs:range bop:Executor ;
                  rdfs:label "implemented by"@en .


###  http://www.github.com/AlexDonkers/bop#implementsProcedure
bop:implementsProcedure rdf:type owl:ObjectProperty ;
                        rdfs:domain bop:Executor ;
                        rdfs:range bop:Procedure ;
                        rdfs:label "implements procedure"@en .


###  http://www.github.com/AlexDonkers/bop#isActedOnBy
bop:isActedOnBy rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain bop:Property ;
                rdfs:range bop:Executor ;
                rdfs:comment "Linking a property with the executor which acts on this property."@en ;
                rdfs:label "is acted on by"@en .


###  http://www.github.com/AlexDonkers/bop#isCollectionOf
bop:isCollectionOf rdf:type owl:ObjectProperty ;
                   rdfs:domain bop:Collection ;
                   rdfs:range bop:Database ;
                   rdfs:comment "Linking a collection with the database it is part of."@en ;
                   rdfs:label "is collection of"@en .


###  http://www.github.com/AlexDonkers/bop#isComplexPropertyOf
bop:isComplexPropertyOf rdf:type owl:ObjectProperty ;
                        rdfs:domain bop:Result ;
                        rdfs:range bop:FeatureOfInterest ;
                        rdfs:comment "Linking a result with a feature of interest"@en ;
                        rdfs:label "is complex property of"@en ;
                        <http://www.w3.org/2004/02/skos/core#example> """This object property is used to directly link the current state of a property to a feature of interest. It is the shortest route to link a result to a feature of interest. 

Example: CurrentWallLength --isComplexPropertyOf --> Wall"""@en .


###  http://www.github.com/AlexDonkers/bop#isExternalDatabaseOf
bop:isExternalDatabaseOf rdf:type owl:ObjectProperty ;
                         rdfs:domain bop:Database ;
                         rdfs:range bop:Executor ;
                         rdfs:label "is external database of"@en .


###  http://www.github.com/AlexDonkers/bop#isFeatureOfInterestOf
bop:isFeatureOfInterestOf rdf:type owl:ObjectProperty ;
                          rdfs:domain bop:FeatureOfInterest ;
                          rdfs:range bop:Execution ;
                          rdfs:label "is feature of interest of"@en .


###  http://www.github.com/AlexDonkers/bop#isHostedBy
bop:isHostedBy rdf:type owl:ObjectProperty ;
               rdfs:domain bop:Executor ;
               rdfs:range bop:Platform ;
               rdfs:label "is hosted by"@en .


###  http://www.github.com/AlexDonkers/bop#isPartOfPropertySet
bop:isPartOfPropertySet rdf:type owl:ObjectProperty ;
                        rdfs:domain bop:Property ;
                        rdfs:range bop:PropertySet ;
                        rdfs:label "is part of property set"@en .


###  http://www.github.com/AlexDonkers/bop#isPropertyOf
bop:isPropertyOf rdf:type owl:ObjectProperty ;
                 rdfs:domain bop:Property ;
                 rdfs:range bop:FeatureOfInterest ;
                 rdfs:label "is property of"@en .


###  http://www.github.com/AlexDonkers/bop#isPropertyStateOf
bop:isPropertyStateOf rdf:type owl:ObjectProperty ;
                      rdfs:domain bop:Result ;
                      rdfs:range bop:Property ;
                      rdfs:comment "Linking a result with a property"@en ;
                      rdfs:label "is property state of"@en ;
                      <http://www.w3.org/2004/02/skos/core#example> """This object property is used to directly link the current state of a property to a property. It is the shortest route to link a result to a property. 

Example: CurrentTemperatureState -- isPropertyStateOf --> Temperature"""@en .


###  http://www.github.com/AlexDonkers/bop#isResultOf
bop:isResultOf rdf:type owl:ObjectProperty ;
               rdfs:domain bop:Result ;
               rdfs:range bop:Execution ;
               rdfs:comment "Linking a result with an execution."@en ;
               rdfs:label "is result of"@en .


###  http://www.github.com/AlexDonkers/bop#isSubActuatorOf
bop:isSubActuatorOf rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf bop:isSubExecutorOf ;
                    rdfs:domain bop:Actuator ;
                    rdfs:range bop:Actuator ;
                    rdfs:comment "Relation between an actuator and sub-actuators. The sub-actuators are of type actuator and could function as individual actuators."@en ;
                    rdfs:label "is sub-actuator of"@en ;
                    <http://www.w3.org/2004/02/skos/core#example> "The object property could be used to model actuators which have multiple components with their own functions."@en .


###  http://www.github.com/AlexDonkers/bop#isSubExecutorOf
bop:isSubExecutorOf rdf:type owl:ObjectProperty ;
                    rdfs:domain bop:Executor ;
                    rdfs:range bop:Executor ;
                    rdfs:comment "Relation between an executor and sub-executors. The sub-executors are of type executor and could function as individual executors."@en ;
                    rdfs:label "is sub-executor of"@en ;
                    <http://www.w3.org/2004/02/skos/core#example> "The object property could be used to model devices which have multiple components with their own functions. A multisensor - for example - consists of multiple sub-sensors which individually perform a sensing function."@en .


###  http://www.github.com/AlexDonkers/bop#isSubSensorOf
bop:isSubSensorOf rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf bop:isSubExecutorOf ;
                  rdfs:domain bop:Sensor ;
                  rdfs:range bop:Sensor ;
                  rdfs:comment "Relation between a sensor and sub-sensors. The sub-sensors are of type sensor and could function as individual sensors."@en ;
                  rdfs:label "is sub-sensor of"@en ;
                  <http://www.w3.org/2004/02/skos/core#example> "The object property could be used to model sensors which have multiple components with their own functions."@en .


###  http://www.github.com/AlexDonkers/bop#isUnitOf
bop:isUnitOf rdf:type owl:ObjectProperty ,
                      owl:InverseFunctionalProperty ;
             rdfs:domain bop:Unit ;
             rdfs:range bop:Result ;
             rdfs:label "is unit of"@en .


###  http://www.github.com/AlexDonkers/bop#isUsedFor
bop:isUsedFor rdf:type owl:ObjectProperty ;
              owl:inverseOf bop:usedProcedure ;
              rdfs:domain bop:Procedure ;
              rdfs:range bop:Execution ;
              rdfs:label "is used for"@en .


###  http://www.github.com/AlexDonkers/bop#observedBy
bop:observedBy rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf bop:executedBy ;
               owl:inverseOf bop:performedObservation ;
               rdfs:domain bop:Observation ;
               rdfs:range bop:Sensor ;
               rdfs:label "observed by"@en .


###  http://www.github.com/AlexDonkers/bop#performedActuation
bop:performedActuation rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf bop:performedExecution ;
                       rdfs:domain bop:Actuator ;
                       rdfs:range bop:Actuation ;
                       rdfs:label "performed actuation"@en .


###  http://www.github.com/AlexDonkers/bop#performedExecution
bop:performedExecution rdf:type owl:ObjectProperty ;
                       rdfs:domain bop:Executor ;
                       rdfs:range bop:Execution ;
                       rdfs:label "performed execution"@en .


###  http://www.github.com/AlexDonkers/bop#performedObservation
bop:performedObservation rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf bop:performedExecution ;
                         rdfs:domain bop:Sensor ;
                         rdfs:range bop:Observation ;
                         rdfs:label "performed observation"@en .


###  http://www.github.com/AlexDonkers/bop#usedProcedure
bop:usedProcedure rdf:type owl:ObjectProperty ;
                  rdfs:domain bop:Execution ;
                  rdfs:range bop:Procedure ;
                  rdfs:label "used procedure"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.github.com/AlexDonkers/bop#hasSimpleProperty
bop:hasSimpleProperty rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain bop:FeatureOfInterest ;
                      rdfs:comment "Linking a feature of interest with a simple property value described as a literal."@en ;
                      rdfs:label "has simple property"@en .


###  http://www.github.com/AlexDonkers/bop#hasSimplePropertyState
bop:hasSimplePropertyState rdf:type owl:DatatypeProperty ,
                                    owl:FunctionalProperty ;
                           rdfs:domain bop:Property ;
                           rdfs:comment "Linking a property with a simple property state value described as a literal."@en ;
                           rdfs:label "has simple property state"@en .


###  http://www.github.com/AlexDonkers/bop#hasSimpleResult
bop:hasSimpleResult rdf:type owl:DatatypeProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain bop:Execution ;
                    rdfs:comment "Linking an execution with a simple result value described as a literal."@en ;
                    rdfs:label "has simple result"@en .


###  http://www.github.com/AlexDonkers/bop#hasSimpleUnit
bop:hasSimpleUnit rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain bop:Result ;
                  rdfs:comment "Linking a result with a unit described by a literal."@en ;
                  rdfs:label "has simple unit"@en .


###  http://www.github.com/AlexDonkers/bop#hasValue
bop:hasValue rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain bop:Result ;
             rdfs:comment "The simple value of a result."@en ;
             rdfs:label "has value"@en .


###  http://www.w3.org/ns/prov#generatedAtTime
prov:generatedAtTime rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain bop:Result .


#################################################################
#    Classes
#################################################################

###  http://www.github.com/AlexDonkers/bop#Actuation
bop:Actuation rdf:type owl:Class ;
              rdfs:subClassOf bop:Execution ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty bop:actuatedBy ;
                                owl:allValuesFrom bop:Actuator
                              ] ;
              rdfs:comment "An act of carrying out an actuation procedure by an actuator to change the state of a property."@en ;
              rdfs:label "Actuation"@en ,
                         "Bediening"@nl .


###  http://www.github.com/AlexDonkers/bop#Actuator
bop:Actuator rdf:type owl:Class ;
             rdfs:subClassOf bop:Executor ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty bop:hasSubActuator ;
                               owl:allValuesFrom bop:Actuator
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty bop:performedActuation ;
                               owl:allValuesFrom bop:Actuation
                             ] ;
             rdfs:comment "An agent that can implement an actuation procedure to change the state of a property.."@en ;
             rdfs:label "Actuator"@en ,
                        "Actuator"@nl .


###  http://www.github.com/AlexDonkers/bop#Collection
bop:Collection rdf:type owl:Class ;
               rdfs:subClassOf bop:Result ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty bop:isCollectionOf ;
                                 owl:allValuesFrom bop:Database
                               ] ;
               rdfs:comment "A collection of documents in a database."@en ;
               rdfs:label "Collectie"@nl ,
                          "Collection"@en .


###  http://www.github.com/AlexDonkers/bop#CurrentState
bop:CurrentState rdf:type owl:Class ;
                 rdfs:subClassOf bop:Result ;
                 rdfs:comment "The most recently defined state of a property."@en ;
                 rdfs:label "Current state"@en ,
                            "Huidige staat"@nl .


###  http://www.github.com/AlexDonkers/bop#Database
bop:Database rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty bop:isExternalDatabaseOf ;
                               owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass bop:Executor
                             ] ;
             rdfs:comment """A collection of data.

The database class is used to describe an external database, storing results of executions, such as a time-series database."""@en ;
             rdfs:label "Databank"@nl ,
                        "Database"@en .


###  http://www.github.com/AlexDonkers/bop#Execution
bop:Execution rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty bop:executedBy ;
                                owl:allValuesFrom bop:Executor
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty bop:implementedBy ;
                                owl:allValuesFrom bop:Procedure
                              ] ;
              rdfs:comment "An act of carrying out a procedure by an executor on a property"@en ,
                           "Het doen of uitvoeren van iets. Een uitvoering kan worden uitgevoerd door een uitvoerder, eventueel op basis van een procedure."@nl ;
              rdfs:label "Execution"@en ,
                         "Uitvoering"@nl .


###  http://www.github.com/AlexDonkers/bop#Executor
bop:Executor rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty bop:actsOn ;
                               owl:allValuesFrom bop:Property
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty bop:hasSubExecutor ;
                               owl:allValuesFrom bop:Executor
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty bop:isHostedBy ;
                               owl:allValuesFrom bop:Platform
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty bop:performedExecution ;
                               owl:allValuesFrom bop:Execution
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty bop:isHostedBy ;
                               owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass bop:Platform
                             ] ;
             rdfs:comment "An agent that can implement a procedure to perform an execution."@en ,
                          "Iets wat een taak kan uitvoeren op basis van een procedure."@nl ;
             rdfs:label "Executor"@en ,
                        "Uitvoerder"@nl .


###  http://www.github.com/AlexDonkers/bop#FeatureOfInterest
bop:FeatureOfInterest rdf:type owl:Class ;
                      rdfs:subClassOf [ rdf:type owl:Restriction ;
                                        owl:onProperty bop:isFeatureOfInterestOf ;
                                        owl:allValuesFrom bop:Property
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty bop:isFeatureOfInterestOf ;
                                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onClass bop:Property
                                      ] ;
                      rdfs:comment "An abstraction of a real-world phenomenon which could be described in terms of its properties."@en ,
                                   "Een abstractie van een fenomeen uit de echte wereld (object, onderdeel, zone, evenement, etc)."@nl ;
                      rdfs:label "Feature of interest"@en ,
                                 "Kenmerk van belang"@nl ;
                      <http://www.w3.org/2004/02/skos/core#example> "An object, part, zone, event etc."@en .


###  http://www.github.com/AlexDonkers/bop#Function
bop:Function rdf:type owl:Class ;
             rdfs:subClassOf bop:Property ;
             rdfs:comment "A function of a feature of interest."@en ;
             rdfs:label "Functie"@nl ,
                        "Function"@en .


###  http://www.github.com/AlexDonkers/bop#Observation
bop:Observation rdf:type owl:Class ;
                rdfs:subClassOf bop:Execution ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty bop:observedBy ;
                                  owl:allValuesFrom bop:Sensor
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty bop:usedProcedure ;
                                  owl:allValuesFrom bop:Procedure
                                ] ;
                rdfs:comment "An act of carrying out an observation procedure by a sensor to observe the state of a property."@en ;
                rdfs:label "Observatie"@nl ,
                           "Observation"@en .


###  http://www.github.com/AlexDonkers/bop#Platform
bop:Platform rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty bop:hosts ;
                               owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass bop:Executor
                             ] ;
             rdfs:comment "An entity that hosts an executor."@en ;
             rdfs:label "Platform"@en ,
                        "Platform"@nl .


###  http://www.github.com/AlexDonkers/bop#Procedure
bop:Procedure rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty bop:implementedBy ;
                                owl:allValuesFrom bop:Executor
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty bop:isUsedFor ;
                                owl:allValuesFrom bop:Execution
