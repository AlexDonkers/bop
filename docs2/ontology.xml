    <!-- http://www.github.com/AlexDonkers/bop#Execution -->


    <owl:Class rdf:about="http://www.github.com/AlexDonkers/bop#Execution">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.github.com/AlexDonkers/bop#executedBy"/>
