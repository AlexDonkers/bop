        <rdfs:label xml:lang="nl">Uitvoering</rdfs:label>
    </owl:Class>
    


    <!-- http://www.github.com/AlexDonkers/bop#Executor -->


    <owl:Class rdf:about="http://www.github.com/AlexDonkers/bop#Executor">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.github.com/AlexDonkers/bop#isHostedBy"/>
                <owl:allValuesFrom rdf:resource="http://www.github.com/AlexDonkers/bop#Platform"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.github.com/AlexDonkers/bop#isHostedBy"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.github.com/AlexDonkers/bop#Platform"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.github.com/AlexDonkers/bop#Platform"/>
        <rdfs:comment xml:lang="en">An agent that can implement a procedure to perform an execution.</rdfs:comment>
        <rdfs:comment xml:lang="nl">Iets wat een taak kan uitvoeren op basis van een procedure.</rdfs:comment>
        <rdfs:label xml:lang="en">Executor</rdfs:label>
        <rdfs:label xml:lang="nl">Uitvoerder</rdfs:label>
    </owl:Class>
    


